{
  "name": "web-frontend",
  "version": "1.0.0",
  "description": "web-frontend-mesosphare-challange",
  "main": "app.js",
  "author": "ach boukhili",
  "license": "ISC",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "nodemon src/server/appServer.js --exec babel-node",
    "dev-server": "webpack-dev-server webpack.config.babel.js --mode development",
    "test:unit": "jest ./src/",
    "build": "npm run clean ; webpack -p ",
    "clean": "./config/scripts/cleanDist.sh",
    "start-prod": "nodemon src/server/appServerIndex.js --exec babel-node",
    "test": "test"
  },
  "keywords": [
    "web",
    "frontend"
  ],
  "jest": {
    "transform": {
      ".*": "babel-jest"
    },
    "globals": {
      "__ENV": true
    },
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/common/tests/__mocks__/fileMock.js",
      "\\.worker.js": "<rootDir>/src/common/tests/__mocks__/workerMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "smooth-dnd": "<rootDir>/src/common/tests/__mocks__/smooth-dnd.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "./config/tests/setupTests.js"
    ]
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.1",
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.0.1",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "colors": "^1.3.3",
    "cookie-parser": "^1.4.3",
    "enzyme": "^3.8.0",
    "express": "^4.17.3",
    "express-session": "^1.15.6",
    "hot": "^0.0.7",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.11.0",
    "opn": "^5.4.0",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "pug": "^2.0.3",
    "radium": "^0.25.1",
    "react": "16.8.1",
    "react-animations": "^1.0.0",
    "react-dom": "16.8.1",
    "react-notifications-component": "^1.0.1",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.8.1",
    "react-toastify": "^4.5.2",
    "uniqid": "^5.0.3",
    "webpack": "^4.29.3",
    "webpack-dev-middleware": "^3.5.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-engine-plugin": "^0.3.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-jest-hoist": "^24.1.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^2.1.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.0",
    "eslint-plugin-react-hooks": "0.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-watch": "^4.0.2",
    "eventsource-polyfill": "^0.9.6",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "react-hot-loader": "^4.6.5",
    "regenerator-runtime": "^0.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack-cli": "^3.2.3"
  }
}
